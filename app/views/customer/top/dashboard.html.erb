<% @Head_Title="顧客ページ" %>

<!--ナビゲーションの作成-->
<%= render 'customer/shared/nav_login' %>
<div class="content content-dashboard">
  <div class="container">
    <div class="flash-area">
      <!--成功した場合-->
      <% if flash[:notice] %>
          <div class="alert alert-success">
            <%= flash[:notice] %>
          </div>
      <% end %>
      <!--失敗した場合-->
      <% if flash[:alert] %>
          <div class="alert alert-warning">
            <%= flash[:alert] %>
          </div>
      <% end %>
    </div>
    <!--タイトル-->
    <h1><%= @Head_Title %></h1>
  </div>
</div>




